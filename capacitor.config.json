{
  "appId": "com.famille.dieulgandet",
  "appName": "FamilyHub",
  "webDir": "www",
  "server": {
    "androidScheme": "https",
    "cleartext": false
  },
  "android": {
    "allowMixedContent": false,
    "backgroundColor": "#080f14",
    "overrideUserAgent": "FamilyHub/2.0 Android",
    "appendUserAgent": "FamilyHubApp"
  },
  "plugins": {
    "StatusBar": {
      "style": "DARK",
      "backgroundColor": "#080f14",
      "overlaysWebView": false
    },
    "SplashScreen": {
      "launchShowDuration": 1200,
      "backgroundColor": "#080f14",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    },
    "Keyboard": {
      "resize": "body",
      "style": "dark",
      "resizeOnFullScreen": true
    }
  }
}
